{
  "name": "webissues",
  "description": "WebIssues team collaboration system",
  "version": "2.0.0",
  "author": "WebIssues Team",
  "private": true,
  "scripts": {
    "build": "npm run build:web && npm run build:electron && npm run build:main",
    "build:web": "webpack --config src/build/webpack.config.js --env.production",
    "build:electron": "webpack --config src/build/webpack.config.js --env.electron --env.production",
    "build:main": "webpack --config app/src/build/webpack.config.js --env.production",
    "dev:web": "webpack-dev-server --config src/build/webpack.config.js",
    "dev:electron": "webpack-dev-server --config src/build/webpack.config.js --env.electron",
    "start": "electron ./app/assets/js/main.min.js",
    "start:dev": "electron ./app/src/main.js",
    "package": "node ./app/src/build/package.js",
    "package:win32": "node ./app/src/build/package.js win32 ia32",
    "package:darwin": "node ./app/src/build/package.js darwin x64",
    "package:linux-ia32": "node ./app/src/build/package.js linux ia32",
    "package:linux-x64": "node ./app/src/build/package.js linux x64",
    "package:server": "node ./src/build/package.js",
    "test": "cross-env NODE_ENV=test mocha --require @babel/register src/test.js src/**/*.spec.js",
    "test:watch": "cross-env NODE_ENV=test mocha --require @babel/register --watch src/test.js src/**/*.spec.js"
  },
  "dependencies": {
    "bootstrap": "^3.4.1",
    "compare-versions": "^3.5.1",
    "core-js": "^3.1.4",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.15.8",
    "path-to-regexp": "^3.0.0",
    "vue": "^2.6.10",
    "vue-i18n": "^8.11.2",
    "vuedraggable": "^2.23.0",
    "vuex": "^3.1.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "archiver": "^3.0.0",
    "assets-webpack-plugin": "^3.9.10",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "chai": "^4.2.0",
    "chai-datetime": "^1.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "electron": "^5.0.5",
    "electron-packager": "^14.0.0",
    "file-loader": "^4.0.0",
    "glob": "^7.1.4",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "mkdirp": "^0.5.1",
    "mocha": "^6.1.4",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  }
}
